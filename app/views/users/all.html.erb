<h3>Usuario(s)</h3>


<div class="container">
	<table class="table table-hover">
		
		<p></p>
		<thead>
		<tr>
		<th></th>
		<th>Nombre:</th>
		<th>Correo:</th></tr>
		</thead>
		<% @users.each do |users| %>
			<tr><td class="round-image-50"><%= image_tag(users.avatar.url(:thumb)) %></td>
			</td><td><%= link_to users.name, user_path(users)%></td>
			<td><%= users.email %></td></tr>
		<%end%>
	</table>
</div>